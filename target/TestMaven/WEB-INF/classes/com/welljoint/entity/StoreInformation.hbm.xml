<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.storeInformation.model.StoreInformationVO" table="dbo.StoreInformation" >
        <id name="id" column="Id">
        <generator class="identity"></generator>
        </id>
        <property name="startNum" column="StartNum" />
        <property name="endNum" column="EndNum" />
        <property name="name" column="Name" />
        <property name="value" column="Value" />
        <property name="store" column="Store" />
        <property name="storeNumber" column="StoreNumber" />
        <property name="storeAddress" column="StoreAddress" />
        <property name="status" column="Status" />
        <property name="storeCode" column="StoreCode" />
    </class>
</hibernate-mapping>
