<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.welljoint.entity.ProductVO" table="dbo.Products" >
        <id name="id" column="Id"> 
        <generator class="identity"></generator>
        </id>
        <property name="productClass" column="productClass" />
        <property name="productClasskey" column="productClasskey" />
        <property name="productionName" column="productionName" />
        <property name="subordinate_Name" column="subordinate_Name" />
        <property name="productionContent0" column="productionContent0" />
        <property name="productionContent1" column="productionContent1" />
        <property name="productionContent2" column="productionContent2" />
        <property name="prices" column="prices" />
        <property name="discountPrice" column="discountPrice" />
        <property name="productImg" column="productImg" />
        <property name="productImg1" column="productImg1" />
        <property name="productExist" column="productExist" />
        <property name="productAmount" column="productAmount" />
        <property name="choice_Item1" column="choice_Item1" />
        <property name="choice_Item2" column="choice_Item2" />
        <property name="choice_Item3" column="choice_Item3" />
        <property name="orderDisplay" column="orderDisplay" />
        <property name="sequence" column="sequence" />
        <property name="calories" column="calories" />
        <property name="week" column="week" />
        <property name="timeInterval" column="timeInterval" />
        <property name="tax_Type" column="tax_Type" />
        <property name="tax_Rate" column="tax_Rate" />
        <property name="description" column="description" />
    </class>
</hibernate-mapping>
